    <script>

        document.querySelector('.detail-quantity .qty-down').addEventListener('click', function () {
            updateQuantity(-1);
        });

        document.querySelector('.detail-quantity .qty-up').addEventListener('click', function () {
            updateQuantity(1);
        });

        function updateQuantity(change) {
            const quantity = parseInt(document.querySelector('.detail-quantity .qty-val').textContent);
            let currentQuantity = parseInt(qtyElement.textContent);
            
            if (currentQuantity + change >= 1) {
                currentQuantity += change;
                qtyElement.textContent = currentQuantity;
            }
        }


        function addToCart(productId) {
            const quantity = parseInt(document.querySelector('.detail-quantity .qty-val').textContent);
            fetch('/cart/add-to-cart', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                },
                body: JSON.stringify({ productId: productId, quantity: quantity }),
            })
                .then(response => {
                    if (response.ok) {
                        Swal.fire("Added to cart successfully!").then(() => {
                            window.location.reload();
                        });
                        console.log('Product added to cart successfully.');
                    } else {
                        console.error('Failed to add product to cart.');
                    }
                })
                .catch(error => {
                    console.error('Error:', error);
                });
        }

        const isWishlisted = document.getElementById('isWishlisted').value.trim();// Get the value from the server-side rendering

        // Select the wishlist icon element
        const wishlistIcon = document.getElementById('wishlist-icon');
        console.log(isWishlisted)

        // Set the initial state of the wishlist icon based on isWishlisted
        if (isWishlisted === 'true') { // Check if isWishlisted is equal to 'true'
            wishlistIcon.classList.add('active');
            wishlistIcon.style.backgroundColor = '#088178';
        }


        // Add to wishlist

        document.getElementById('wishlist-icon').addEventListener('click', function (event) {
            event.preventDefault(); // Prevent default action of clicking a link

            const wishlistIcon = document.getElementById('wishlist-icon');
            const isProductInWishlist = wishlistIcon.classList.contains('active');

            // Fetch request
            fetch(isProductInWishlist ? '/home/remove-from-wishlist' : '/home/add-to-wishlist', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    productId: '<%= product._id %>'
                })
            })
                .then(response => response.json()) // Parse response as JSON
                .then(data => {
                    if (data.wishlist) {
                        if (isProductInWishlist) {
                            // Remove product from wishlist
                            wishlistIcon.classList.remove('active');
                            wishlistIcon.style.backgroundColor = ''; // Reset background color if removed from wishlist
                            // Show success message
                            Swal.fire({
                                position: "top-end",
                                icon: "success",
                                title: "Product removed from wishlist",
                                showConfirmButton: false,
                                timer: 1500
                            });
                        } else {
                            // Add product to wishlist
                            wishlistIcon.classList.add('active');
                            wishlistIcon.style.backgroundColor = '#088178';
                            // Show success message
                            Swal.fire({
                                position: "top-end",
                                icon: "success",
                                title: "Product added to wishlist",
                                showConfirmButton: false,
                                timer: 1500
                            });
                        }

                        // Update wishlist count dynamically
                        document.querySelector('.pro-count.blue').textContent = data.wishlist.items.length;
                        console.log(data.wishlist.items.length);
                    }
                })
                .catch(error => {
                    console.error('Error:', error);
                });
        });






    </script>
